	<screen name="MergeLeads">
		<section>
			<actions>
				<set field="headerItem" value="sfa" />
				<set field="tabButtonItem" value="Leads" />
				<set field="tabButtonItem1" value="listLeads" />
				<set field="tabButtonItem2" value="MergeLeads" />
				<set field="secondaryMenuName" value="LeadTabBar" global="true" />
				<set field="secondaryMenuLocation" value="component://marketing/widget/sfa/SfaMenus.xml"
					global="true" />
				<script
					location="component://marketing/webapp/sfa/WEB-INF/action/MergeContacts.groovy" />
			</actions>
			<widgets>
				<decorator-screen name="main-decorator"
					location="${parameters.mainDecoratorLocation}">
					<decorator-section name="left-column">
						<include-screen name="leftbar"
							location="component://marketing/widget/sfa/CommonScreens.xml" />
					</decorator-section>
					<decorator-section name="body">
						<include-menu location="component://marketing/widget/sfa/SfaMenus.xml"
							name="LeadTabBar" />
						<screenlet title="Merge Companies">
							<include-form name="MergeLeads"
								location="component://marketing/widget/sfa/forms/LeadForms.xml" />
						</screenlet>
						<section>
							<condition>
								<if-compare-field field="parameters.partyIdFrom"
									operator="not-equals" to-field="parameters.partyIdTo" />
							</condition>
							<widgets>
								<platform-specific>
									<html>
										<html-template
											location="component://marketing/webapp/sfa/lead/mergeLeads.ftl" />
									</html>
								</platform-specific>
							</widgets>
							<fail-widgets>
								<label text="${uiLabelMap.SfaCanNotMergeSameLeads}" style="h3"></label>
							</fail-widgets>
						</section>
					</decorator-section>
				</decorator-screen>
			</widgets>
		</section>
	</screen>

