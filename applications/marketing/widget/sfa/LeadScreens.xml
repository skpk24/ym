												<!-- <fail-widgets> list my leads <label style="h1" text="${uiLabelMap.SfaMyLeads}"/> 
													<include-form name="ListMyLeads" location="component://marketing/widget/sfa/forms/LeadForms.xml"/> 
													</fail-widgets>
-->
											</section>
										</decorator-section>
									</decorator-screen>
								</container>
							</widgets>
						</section>
					</decorator-section>
				</decorator-screen>
			</widgets>
		</section>
	</screen>


